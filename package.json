{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/bin/www",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www",
    "local": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/bin/www",
    "watch": "nodemon",
    "heroku-prebuild": "npm install -f",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "async": "^3.2.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "kue": "^0.11.6",
    "mongoose": "^5.12.13",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.13",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^6.2.3",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.10.3",
    "case-sensitive-paths-webpack-plugin": "^2.3.0"
  },
  "engines": {
    "node": "12.18.3"
  }
}
